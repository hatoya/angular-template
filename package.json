{
  "name": "angular-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "emulator": "firebase emulators:start",
    "start": "ng serve & npm run emulator",
    "build": "ng build --aot --configuration production",
    "test": "ng test --watch",
    "test:cli": "ng test --no-watch",
    "prettier": "prettier --write 'src/**/*.*'",
    "tslint": "tslint './src/**/*.ts' --fix",
    "lint": "npm run prettier && npm run tslint",
    "e2e": "ng e2e",
    "deploy": "firebase deploy --only hosting,firestore:rules",
    "component": "func() { ng g component component/\"$1\"/\"$2\" && ng g service component/\"$1\"/\"$2\"/\"$2\" && npm run lint; }; func",
    "page": "func() { ng g component component/page/\"$1\" && ng g af component/page/\"$1\"/\"$1\" --spec --plain && npm run lint; }; func",
    "modal": "func() { ng g component component/modal/\"$1\" && ng g af component/modal/\"$1\"/\"$1\" --spec --plain && npm run lint; }; func",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook --ci -p 7777",
    "build:storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.15",
    "@angular/common": "~12.2.15",
    "@angular/compiler": "~12.2.15",
    "@angular/core": "~12.2.15",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~12.2.15",
    "@angular/platform-browser": "~12.2.15",
    "@angular/platform-browser-dynamic": "~12.2.15",
    "@angular/router": "~12.2.15",
    "@datorama/akita": "^6.0.0",
    "@datorama/akita-ng-entity-service": "^6.0.0",
    "@fortawesome/angular-fontawesome": "~0.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.2",
    "@fortawesome/pro-regular-svg-icons": "^5.15.2",
    "@fortawesome/pro-solid-svg-icons": "^5.15.2",
    "@ngneat/reactive-forms": "^1.7.2",
    "@sentry/angular": "^5.30.0",
    "@sentry/tracing": "^5.30.0",
    "firebase": "^8.2.3",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1202.15",
    "@angular-devkit/build-angular": "~12.2.15",
    "@angular/cli": "~12.2.15",
    "@angular/compiler-cli": "~12.2.15",
    "@angular/language-service": "~12.2.15",
    "@babel/core": "^7.12.10",
    "@compodoc/compodoc": "^1.1.11",
    "@datorama/akita-ng-router-store": "^6.0.0",
    "@datorama/akita-ngdevtools": "^6.0.0",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/angular": "^6.1.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.0",
    "firebase-tools": "^9.2.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}