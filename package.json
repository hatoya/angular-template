{
  "private": true,
  "name": "angular-template",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "start:functions": "cd functions && npm start",
    "start:emulator": "npm run build:functions && npm run kill && npm run emulator",
    "kill": "lsof -t -i:9099 -i:5001 -i:8080 -i:5001 -i:9199 -i:8085 -i:4444 | xargs kill -9",
    "emulator": "firebase emulators:start --import ./saved_data --only auth,firestore,storage,functions --project default",
    "emulator:export": "firebase emulators:export ./saved_data --force --project default",
    "emulator:import": "firebase emulators:import ./saved_data --project default",
    "build:functions": "cd functions && npm run build",
    "build:prod": "ng build -c production",
    "build:dev": "ng build -c development",
    "test": "jest service.spec.ts --watch",
    "test:cli": "jest service.spec.ts",
    "prettier": "prettier 'src/**/*.*' --write --loglevel warn",
    "eslint": "eslint './src/**/*.ts' --fix",
    "stylelint": "stylelint './src/**/*.scss' --fix",
    "lint": "npm run prettier && npm run eslint && npm run stylelint",
    "e2e": "ng e2e",
    "firestore:indexes:export": "firebase firestore:indexes > firestore.indexes.json",
    "deploy": "firebase deploy --only hosting,functions,firestore:rules,firestore:indexes",
    "component:page": "func() { ng g component component/page/\"$1\" --skip-tests && ng g service component/page/\"$1\"/\"$1\" && npm run lint; }; func",
    "component:modal": "func() { ng g component component/modal/\"$1\" --skip-tests && ng g service component/modal/\"$1\"/\"$1\" && npm run lint; }; func",
    "component:template": "func() { ng g component component/template/\"$1\" --skip-tests && ng g service component/template/\"$1\"/\"$1\" && npm run lint; }; func",
    "component:molecule": "func() { ng g component component/molecule/\"$1\" --skip-tests && ng g service component/molecule/\"$1\"/\"$1\" && npm run lint; }; func",
    "component:atom": "func() { ng g component component/atom/\"$1\" --skip-tests && ng g service component/atom/\"$1\"/\"$1\" && npm run lint; }; func",
    "service": "func() { ng g service service/\"$1\"  && npm run lint; }; func",
    "pipe": "func() { ng g pipe pipe/\"$1\"  && npm run lint; }; func",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d ."
  },
  "workspaces": [
    "functions"
  ],
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@fortawesome/angular-fontawesome": "~0.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/pro-duotone-svg-icons": "^6.4.2",
    "@fortawesome/pro-regular-svg-icons": "^6.4.2",
    "@fortawesome/pro-solid-svg-icons": "^6.4.2",
    "firebase": "^9.12.0",
    "postcss-scss": "^4.0.5",
    "rxjs": "~6.6.7",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-scss": "^4.3.0",
    "superstruct": "^1.0.3",
    "tslib": "^2.0.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1602.0",
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/language-service": "^16.2.0",
    "@babel/core": "^7.19.3",
    "@compodoc/compodoc": "^1.1.19",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "babel-loader": "^8.2.2",
    "eslint": "^8.25.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "firebase-tools": "^12.4.7",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "^29.6.2",
    "jest-preset-angular": "^13.1.1",
    "ng-packagr": "^16.2.0",
    "prettier": "^2.7.1",
    "protractor": "~7.0.0",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-prettier": "^2.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.9.5"
  }
}
