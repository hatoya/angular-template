{
  "private": true,
  "name": "angular-template",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "start:functions": "cd functions && npm start",
    "start:emulator": "npm run build:functions && npm run kill && npm run emulator",
    "kill": "lsof -t -i:9099 -i:5001 -i:8080 -i:5001 -i:9199 -i:8085 -i:4444 | xargs kill -9",
    "emulator": "firebase emulators:start --import ./saved_data --only auth,firestore,storage,functions --project default",
    "emulator:export": "firebase emulators:export ./saved_data --force --project default",
    "emulator:import": "firebase emulators:import ./saved_data --project default",
    "build:functions": "cd functions && npm run build",
    "build:prod": "ng build -c production",
    "build:dev": "ng build -c development",
    "build:storybook": "npm run docs:json && build-storybook",
    "test": "jest service.spec.ts --watch",
    "test:cli": "jest service.spec.ts",
    "prettier": "prettier 'src/**/*.*' --write --loglevel warn",
    "eslint": "eslint './src/**/*.ts' --fix",
    "lint": "npm run prettier && npm run eslint",
    "e2e": "ng e2e",
    "firestore:indexes:export": "firebase firestore:indexes > firestore.indexes.json",
    "deploy": "firebase deploy --only hosting,functions,firestore:rules,firestore:indexes",
    "component:page": "func() { ng g component component/page/\"$1\" --skip-tests && ng g af component/page/\"$1\"/\"$1\" --spec --plain --defaults && npm run lint; }; func",
    "component:modal": "func() { ng g component component/modal/\"$1\" --skip-tests && ng g af component/modal/\"$1\"/\"$1\" --spec --plain --defaults && npm run lint; }; func",
    "component:template": "func() { ng g component component/template/\"$1\" --skip-tests && ng g af component/template/\"$1\"/\"$1\" --spec --plain --defaults && npm run lint; }; func",
    "component:molecule": "func() { ng g component component/molecule/\"$1\" --skip-tests && ng g af component/molecule/\"$1\"/\"$1\" --spec --plain --defaults && npm run lint; }; func",
    "component:atom": "func() { ng g component component/atom/\"$1\" --skip-tests && ng g af component/atom/\"$1\"/\"$1\" --spec --plain --defaults && npm run lint; }; func",
    "service": "func() { ng g service service/\"$1\"  && npm run lint; }; func",
    "pipe": "func() { ng g pipe pipe/\"$1\"  && npm run lint; }; func",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook --ci -p 7777"
  },
  "workspaces": [
    "functions"
  ],
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/fire": "^7.2.1",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@datorama/akita": "^7.1.1",
    "@datorama/akita-ng-entity-service": "^7.0.0",
    "@fortawesome/angular-fontawesome": "~0.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/pro-duotone-svg-icons": "^6.1.1",
    "@fortawesome/pro-regular-svg-icons": "^6.1.1",
    "@fortawesome/pro-solid-svg-icons": "^6.1.1",
    "@ngneat/reactive-forms": "^4.0.4",
    "@sentry/angular": "^7.0.0",
    "@sentry/tracing": "^7.0.0",
    "firebase": "^9.8.2",
    "rxjs": "~6.6.7",
    "superstruct": "^0.16.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1400.0",
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/language-service": "^14.0.0",
    "@babel/core": "^7.12.10",
    "@compodoc/compodoc": "^1.1.11",
    "@datorama/akita-ng-router-store": "^7.0.0",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@ngneat/eslint-plugin-reactive-forms": "^4.0.0",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/angular": "^6.1.14",
    "@types/jest": "^28.1.1",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "babel-loader": "^8.2.2",
    "eslint": "^8.17.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "firebase-tools": "^11.0.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "^28.1.0",
    "jest-preset-angular": "^12.1.0",
    "ng-packagr": "^14.0.0",
    "prettier": "^2.6.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.7.3"
  }
}
