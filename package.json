{
  "private": true,
  "name": "angular-template",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "start:functions": "cd functions && npm start",
    "start:emulator": "npm run build:functions && npm run kill && npm run emulator",
    "kill": "lsof -t -i:9099 -i:5001 -i:8080 -i:5001 -i:9199 -i:8085 -i:4444 | xargs kill -9",
    "emulator": "firebase emulators:start --import ./saved_data --only auth,firestore,storage,functions --project default",
    "emulator:export": "firebase emulators:export ./saved_data --force --project default",
    "emulator:import": "firebase emulators:import ./saved_data --project default",
    "build:functions": "cd functions && npm run build",
    "build:prod": "ng build -c production",
    "build:dev": "ng build -c development",
    "test": "ng test --watch",
    "test:cli": "ng test --no-watch",
    "prettier": "prettier 'src/**/*.*' --write --loglevel warn",
    "eslint": "eslint './src/**/*.ts' --fix",
    "lint": "npm run prettier && npm run eslint",
    "e2e": "ng e2e",
    "deploy": "firebase deploy --only hosting,functions,firestore:rules",
    "component:page": "func() { ng g component component/page/\"$1\" && ng g af component/page/\"$1\"/\"$1\" --spec --plain --defaults && npm run lint; }; func",
    "component:modal": "func() { ng g component component/modal/\"$1\" && ng g af component/modal/\"$1\"/\"$1\" --spec --plain --defaults && npm run lint; }; func",
    "component:template": "func() { ng g component component/template/\"$1\" && ng g af component/template/\"$1\"/\"$1\" --spec --plain --defaults && npm run lint; }; func",
    "component:molecule": "func() { ng g component component/molecule/\"$1\" && ng g af component/molecule/\"$1\"/\"$1\" --spec --plain --defaults && npm run lint; }; func",
    "component:atom": "func() { ng g component component/atom/\"$1\" && ng g af component/atom/\"$1\"/\"$1\" --spec --plain --defaults && npm run lint; }; func",
    "service": "func() { ng g service service/\"$1\"  && npm run lint; }; func",
    "pipe": "func() { ng g pipe pipe/\"$1\"  && npm run lint; }; func",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook --ci -p 7777",
    "build:storybook": "npm run docs:json && build-storybook"
  },
  "workspaces": [
    "functions"
  ],
  "dependencies": {
    "@angular/animations": "~13.1.2",
    "@angular/common": "~13.1.2",
    "@angular/compiler": "~13.1.2",
    "@angular/core": "~13.1.2",
    "@angular/fire": "^7.2.0",
    "@angular/forms": "~13.1.2",
    "@angular/platform-browser": "~13.1.2",
    "@angular/platform-browser-dynamic": "~13.1.2",
    "@angular/router": "~13.1.2",
    "@datorama/akita": "^7.1.1",
    "@datorama/akita-ng-entity-service": "^7.0.0",
    "@fortawesome/angular-fontawesome": "~0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.4",
    "@fortawesome/pro-regular-svg-icons": "^5.15.4",
    "@fortawesome/pro-solid-svg-icons": "^5.15.4",
    "@ngneat/reactive-forms": "^1.7.5",
    "@sentry/angular": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "firebase": "^9.6.3",
    "rxjs": "~6.6.7",
    "superstruct": "^0.15.3",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1301.3",
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "~13.1.2",
    "@angular/language-service": "~13.1.2",
    "@babel/core": "^7.12.10",
    "@compodoc/compodoc": "^1.1.11",
    "@datorama/akita-ng-router-store": "^7.0.0",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/angular": "^6.1.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "babel-loader": "^8.2.2",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "firebase-tools": "^10.1.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.0.0",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.5.4"
  }
}
